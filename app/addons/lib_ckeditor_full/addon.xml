	<priority>10</priority>
	<position>0</position>
	<status>active</status>
	<tab_order>prepend</tab_order>
	<default_language>en</default_language>
	<compatibility>
		<core_version>
			<min>4.3.4</min>
			<!-- Untested below v4.3.4 -->
		</core_version>
	</compatibility>
	<authors>
		<author>
			<name>Dhaupin - CreadevDotOrg</name>
			<email>dhaupin@creadev.org</email>
			<url>https://creadev.org</url>
		</author>
	</authors>

	<language_variables>
		<item lang="en" id="tmpl_text_ckeditor_full">CKEditor Full</item>
	</language_variables>
	<functions>
		<item for="install">fn_lib_ckeditor_full_install</item>
		<item for="uninstall">fn_lib_ckeditor_full_uninstall</item>
	</functions>
</addon>
