<?xml version="1.0"?>
<addon scheme="2.0" edition_type="ROOT,ULT:VENDOR">
	<id>lib_ckeditor_full</id>
	<version>1.1.0</version>
	<name>Library - WYSIWYG CKEditor Full</name>
